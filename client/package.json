    "source-map-loader": "^5.0.0",
    "tailwindcss": "^3.3.3",
    "web-vitals": "^4.2.4",
    "webpack-dev-server": "^4.15.2"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "cross-env NODE_ENV=development BABEL_ENV=development eslint --fix",
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
